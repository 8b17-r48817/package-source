pkgname=athena-vim-config
pkgver=1.0.0
pkgrel=1
pkgdesc="Vim configuration for Athena."
arch=('any')
license=("custom")
depends=('vim')
source=("$pkgname.tar.gz")

package() {
  mkdir -p "${pkgdir}/etc/skel"

  cp -rf "${srcdir}/etc/skel/.vimrc" "${pkgdir}/etc/skel/"
  
}
sha512sums=('7760bf56adbcf5aa1fa4625efd01953ef72707a370ee11680ab6a02c35577468e9f2594913ae1e7bb0504cf91cca3e13e8fbb186fbfc606fcc48630515d4dcfb')
