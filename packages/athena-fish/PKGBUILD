pkgname=athena-fish
pkgver=1.0.0
pkgrel=3
pkgdesc="Fish for Athena"
arch=('any')
license=("custom")
depends=('fish')
source=("$pkgname.tar.gz")

package() {
  mkdir -p "${pkgdir}/etc/skel/.config"

  cp -rf "${srcdir}/etc/skel/.config/fish" "${pkgdir}/etc/skel/.config/"  
}

sha256sums=('fa89abac9e76cb61120b3fcbc0e3a780b7706bd1b2bbbe832d7f5e12968ac210')
