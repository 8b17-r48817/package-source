pkgname=athena-fish
pkgver=1.0.0
pkgrel=3
pkgdesc="Fish for Athena"
arch=('any')
license=("custom")
depends=('fish')
source=("$pkgname.tar.gz")

package() {
  mkdir -p "${pkgdir}/etc/skel/.config"

  cp -rf "${srcdir}/etc/skel/.config/fish" "${pkgdir}/etc/skel/.config/"  
}

sha256sums=('d895457507f0f852314ca96fc024a6a59f0f245d831d9e611235f935563a4825')
