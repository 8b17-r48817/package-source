pkgname=athena-fish
pkgver=1.0.0
pkgrel=3
pkgdesc="Fish for Athena"
arch=('any')
license=("custom")
depends=('fish')
source=("$pkgname.tar.gz")

package() {
  mkdir -p "${pkgdir}/etc/skel/.config"

  cp -rf "${srcdir}/etc/skel/.config/fish" "${pkgdir}/etc/skel/.config/"  
}

sha256sums=('ad51d58b6236d7f61304dd4a65dd1dc35703ffb0819c30c735ce5f188ef5b5bb')
