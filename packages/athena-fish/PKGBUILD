pkgname=athena-fish
pkgver=1.0.0
pkgrel=3
pkgdesc="Fish for Athena"
arch=('any')
license=("custom")
depends=('fish')
source=("$pkgname.tar.gz")

package() {
  mkdir -p "${pkgdir}/etc/skel/.config"

  cp -rf "${srcdir}/etc/skel/.config/fish" "${pkgdir}/etc/skel/.config/"  
}

sha256sums=('50bba5efbdb7eae88fb3669830a51b4d44f093dec2867bbb85ab2a9948e231cc')
