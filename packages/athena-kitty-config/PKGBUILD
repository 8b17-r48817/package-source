pkgname=athena-kitty-config
pkgver=1.0.0
pkgrel=1
pkgdesc="Kitty configuration for Athena."
arch=('any')
license=("custom")
depends=('kitty')
source=("$pkgname.tar.gz")

package() {
  mkdir -p "${pkgdir}/etc/skel"

  cp -rf "${srcdir}/etc/skel/.config" "${pkgdir}/etc/skel/"
}
sha512sums=('16922a2af86416c8a09e5badec348a05850e2ebb2368a7af671bb0e43bc9f147170f085dade5574985c112801f1a2f86e35a6094d591cd55a18668a0d35712e3')
