pkgname=athena-mirrorlist
pkgver=1.0.0
pkgrel=2
pkgdesc="Athena OS mirrorlist."
arch=('any')
license=("custom")
backup=('etc/pacman.d/athena-mirrorlist')
source=("athena-mirrorlist")

package() {
  mkdir -p "$pkgdir/etc/pacman.d"
  
  install -Dm 644 -t "$pkgdir/etc/pacman.d/" $pkgname
}

sha512sums=('f03cd92577eb1424c7c1cb6e3eff790f02b7f81b898659d14b590d28788de50f34f2f68fdadd5dab59bcf9ddbbfb502999bf6bd58c32e59111677c7979617c13')
