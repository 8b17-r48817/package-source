pkgname=athena-mirrorlist
pkgver=1.0.0
pkgrel=1
pkgdesc="Mirrorlist for Athena"
arch=('any')
license=("custom")
source=("athena-mirrorlist")

package() {
  mkdir -p "${pkgdir}/etc/pacman.d"

  cp -rf "${srcdir}"/athena-mirrorlist "${pkgdir}/etc/pacman.d/"
}

