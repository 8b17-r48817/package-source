[Desktop Entry]
Name=Empire
Encoding=UTF-8
Exec=gnome-terminal -- /usr/bin/bash -c "if command -v empire &> /dev/null;then empire -h;$SHELL;else echo \\"Empire is not installed. I'm retrieving it for you...\\";sudo pacman -S empire;empire -h;$SHELL;fi;"
Icon=kali-powershell-empire
StartupNotify=false
Terminal=false
Type=Application
Categories=10-maintaining-access;
X-BlackArch-Package=empire
