[Desktop Entry]
Name=ike-scan
Comment=Discover and fingerprint IKE hosts (IPsec VPN servers)
Encoding=UTF-8
Exec=NO_REPETITION=1 gnome-terminal -- /usr/bin/bash -c "if command -v ike-scan &> /dev/null;then ike-scan -h;$SHELL;else echo \\"ike-scan is not installed. I'm retrieving it for you...\\";sudo pacman -S ike-scan;ike-scan -h;$SHELL;fi;"
