[Desktop Entry]
Name=snmp-check
Comment=Check hosts SNMP access
Encoding=UTF-8
Exec=NO_REPETITION=1 gnome-terminal -- /usr/bin/bash -c "if command -v snmpcheck-nothink &> /dev/null;then snmpcheck-nothink -h;$SHELL;else echo \\"snmpcheck is not installed. I'm retrieving it for you...\\";sudo pacman -S snmpcheck;snmpcheck-nothink -h;$SHELL;fi;"
Icon=kali-snmp-check
StartupNotify=false
Terminal=false
Type=Application
Categories=01-12-snmp-analysis;
X-BlackArch-Package=snmpcheck
