[Desktop Entry]
Name=rsmangler
Encoding=UTF-8
Exec=NO_REPETITION=1 gnome-terminal -- /usr/bin/bash -c "if command -v rsmangler &> /dev/null;then rsmangler -h;$SHELL;else echo \\"rsmangler is not installed. I'm retrieving it for you...\\";sudo pacman -S rsmangler;rsmangler -h;$SHELL;fi;"
Icon=kali-rsmangler
