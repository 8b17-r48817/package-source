[Desktop Entry]
Name=vinetto
Comment=Extract thumbnails and associated metadata from Thumbs.db files
Encoding=UTF-8
Exec=gnome-terminal -- /usr/bin/bash -c "if command -v vinetto &> /dev/null;then vinetto -h;$SHELL;else echo \\"vinetto is not installed. I'm retrieving it for you...\\";sudo pacman -S vinetto;vinetto -h;$SHELL;fi;"
Icon=kali-menu
StartupNotify=false
Terminal=false
Type=Application
Categories=11-04-forensic-analysis-tools;
X-BlackArch-Package=vinetto
