pkgname=athena-gdm-config
pkgver=1.0.0
pkgrel=1
pkgdesc="GDM configuration for Athena"
arch=('any')
depends=('gdm')
license=("custom")
install=gresource.install
source=("$pkgname.tar.gz")

package() {
  mkdir -p "${pkgdir}/usr/share/gnome-shell"

  cp -rf "${srcdir}/usr/share/gnome-shell/gnome-shell-theme.gresource.new" "${pkgdir}/usr/share/gnome-shell/"
  
}
sha512sums=('6aa461ceac1e66f3cd34fe19e745b36c32f75a8ecb331decee281b25f692ec0356629aeeeffd3b4a9905a0bdb63a9ff218360240c304a65f979f7c2790fdf97c')
