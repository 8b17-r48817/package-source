pkgname=athena-neofetch-config
pkgver=1.0.0
pkgrel=1
pkgdesc="Neofetch configuration for Athena"
arch=('any')
license=("custom")
depends=('neofetch')
source=("$pkgname.tar.gz")

package() {
  mkdir -p "${pkgdir}/etc/skel"

  cp -rf "${srcdir}/etc/skel/.config" "${pkgdir}/etc/skel/"
}
